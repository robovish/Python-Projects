-- ----------Create Db--------
CREATE DATABASE finance;

------Master Tabl for all Stocks in World----------

DROP TABLE master_world_stocks_list;

SELECT count(*) FROM master_world_stocks_list where COUNTRY = 'INDIA'; and SYMBOL like ('TCS');

CREATE TABLE master_world_stocks_list
( 
SYMBOL VARCHAR(MAX),
NAME VARCHAR(MAX),
FULL_NAME VARCHAR(MAX),
ISIN VARCHAR(MAX),
CURRENCY VARCHAR(MAX),
COUNTRY VARCHAR(MAX),
CREATED_DT DATE
);

CREATE TABLE watchlist_notes
( 
SYMBOL VARCHAR(MAX),
NOTES VARCHAR(MAX),
CREATED_DT DATE
);

CREATE TABLE exclusion_inclusion_list
( 
LIST_TYPE VARCHAR(100),
SYMBOL VARCHAR(MAX),
CREATED_DT DATE
);

CREATE TABLE india_symbol_update_tracker
( 
UPDATE_TYPE VARCHAR(50),
OLD_SYMBOL VARCHAR(30),
NEW_SYMBOL VARCHAR(30),
ISIN VARCHAR(30),
UPDATE_DT DATE,
CREATED_DT DATE
);


CREATE TABLE intraday_history1
( 
SYMBOL VARCHAR(30),
DATE DATETIME,
OPEN_PRICE FLOAT,
HIGH_PRICE FLOAT,
LOW_PRICE FLOAT,
CLOSE_PRICE FLOAT,
VOLUME BIGINT,
DURATION INT,
CREATED_DT DATE
);

CREATE TABLE intraday_tick_data
( 
SYMBOL VARCHAR(30),
DATE DATETIME,
LTP FLOAT,
OPEN_PRICE FLOAT,
HIGH_PRICE FLOAT,
LOW_PRICE FLOAT,
CLOSE_PRICE FLOAT,
VOLUME BIGINT,
LTQ INT,
TOTAL_BID_QTY INT,
TOTAL_ASK_QTY INT
);
